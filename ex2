package com.mycompany.projetoferias;

import java.util.Observable;

public class Epoca extends Observable{
    
    private int edicao;
    
    public void setNovaEdicao(int novaEdicao){
        this.edicao = novaEdicao;
        
        setChanged();
        notifyObservers();
        
    }
    
    public int getEdicao(){
        return this.edicao;
    }
    
}
